<%= article.likes.count %><%= (article.likes.count) < 2 ? " Like" : " Likes" %>
<% pre_like = article.likes.find { |like| like.user_id == session[:user_id] } %>
<% if pre_like %>
    <%= button_to 'Unlike', article_like_path(article, pre_like), method: :delete %>
<% else %>
    <%= button_to 'Like', article_likes_path(article), method: :post %>
<% end %>